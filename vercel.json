{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    
    { 
      "src": "/(index\\.html|package\\.json|package-lock\\.json|vercel\\.json|.*\\.js|.*\\.css|.*\\.log)",
      "dest": "/$1"
    },
   
    { 
      "src": "/api/(.*)",
      "dest": "/server.js" 
    },
   
    { 
      "src": "/.*", 
      "dest": "/server.js" 
    }
  ]
}